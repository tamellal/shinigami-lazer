{% extends 'layouts/admin.html.twig' %}
{% set active = 'update' %}
{% block page_title %} title.admin.club.update {% endblock %}

{% block content %}
    <h2 class="text-center">{% trans %}admin.club.update{% endtrans %}</h2>
    <div class="row">
        <div class="col-sm-12">
            {{ form_start(form) }}
            <div class="form-row">
                <div class="form-group col-md-6">
                    {{ form_label(form.code) }}
                    {{ form_widget(form.code) }}
                </div>
                <div class="form-group col-md-6">
                    {{ form_label(form.address) }}
                    {{ form_widget(form.address) }}
                </div>
            </div>
            <div class="form-group col-12 text-center">
                {{ form_widget(form.submit) }}
            </div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}

