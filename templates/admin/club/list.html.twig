{% extends 'layouts/admin.html.twig' %}

{% block content %}

    <h2 class="text-center">{% trans %}admin.club.list{% endtrans %}</h2>

    <div class="form-group">
        <ul class="list-group">
            {% for club in clubs %}
                <li class="list-group-item">Code: {{ club.code }}    ==>        Adresse: {{ club.adress }}</li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}

{% block page_title %}
    Liste des clubs
{% endblock %}

